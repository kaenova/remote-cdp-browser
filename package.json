{
  "name": "remote-cdp-browser",
  "version": "1.0.0",
  "description": "Chrome DevTools Protocol proxy server with Bun",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "start": "bun run dist/index.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["chrome", "cdp", "devtools", "proxy", "bun"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "bun-types": "^1.2.22",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "bun": "latest"
  }
}